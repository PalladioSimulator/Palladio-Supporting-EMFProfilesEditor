<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="emfprofiles.feature"
      label="EMF Profiles Graphical Editor"
      version="5.2.2">

   <description url="https://github.com/kit-sdq/EMF-Profiles-Sirius-Editor">
      This is a Sirius-based graphical editor for EMF Profiles. The goal of this graphical editor is to offer a more modern alternative to the default EMF Profiles GMF-based graphical editor, which hasn&apos;t been maintained for several years.
   </description>

   <url>
      <discovery label="EMF Profiles article" url="https://modeling-languages.com/emf-profiles-annotation-mechanism-emf-models/"/>
      <discovery label="EMF Profiles paper" url="http://www.modelversioning.org/images/amor/prototypes/EMFProfiles/emf_profiles_paper.pdf"/>
      <discovery label="EMF Profiles fork" url="https://github.com/PalladioSimulator/emf-profiles"/>
      <discovery label="EMF Profiles original repository" url="https://github.com/planger/emf-profiles"/>
      <discovery label="EMF Profiles Google Code webpage" url="https://code.google.com/archive/a/eclipselabs.org/p/emf-profiles"/>
      <discovery label="EMF Profiles fork update site" url="https://sdqweb.ipd.kit.edu/eclipse/emf-profiles/releases/latest"/>
   </url>

   <requires>
      <import feature="org.modelversioning.emfprofile.feature" version="1.0.0.201710240840"/>
   </requires>

   <plugin
         id="emfprofiles.design"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="emfprofiles.custom"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

</feature>
